{
	"schemaVersion": 1,
	"id": "mccource",
	"version": "${version}",
	"name": "Mod Cource Mod",
	"description": "This is an example description! Tell everyone what your mod is about!",
	"authors": [
		"Owensaug"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "MIT",
	"icon": "assets/modteste/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
          "net.owensaug.mccource.MCCourceMod"
		],
		"fabric-datagen": [
			"net.owensaug.mccource.MCCourceModDataGenerator"
		],
		"client": [
			"net.owensaug.mccource.MCCourceModClient"
		]
	},
	"mixins": [
		"modteste.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.15.9",
		"minecraft": "~1.20.4",
		"java": ">=17",
		"fabric-api": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}